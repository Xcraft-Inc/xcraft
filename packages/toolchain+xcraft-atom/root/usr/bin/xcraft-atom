#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../share/toolchain/xcraft-atom/bin/xcraft-atom" "$@"
  ret=$?
else
  node  "$basedir/../share/toolchain/xcraft-atom/bin/xcraft-atom" "$@"
  ret=$?
fi
exit $ret
