{
  "list": {
    "desc": "display available runners"
  },
  "start": {
    "desc": "start a gitlab-ci runner",
    "options": {
      "params": {
        "required": "id"
      }
    }
  },
  "stop": {
    "desc": "stop a gitlab-ci runner",
    "options": {
      "params": {
        "required": "id"
      }
    }
  },
  "create": {
    "desc": "create and register a new gitlab runner",
    "options": {
      "wizard": true,
      "params": {
        "optional": "token"
      }
    }
  },
  "create.prepare": {
    "parallel": true
  },
  "create.runner": {
    "parallel": true
  },
  "delete": {
    "desc": "delete and unregister a gitlab runner",
    "options": {
      "params": {
        "required": "id"
      }
    }
  }
}
